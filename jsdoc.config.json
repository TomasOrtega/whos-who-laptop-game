{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "server",
      "client/js"
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true
  }
}